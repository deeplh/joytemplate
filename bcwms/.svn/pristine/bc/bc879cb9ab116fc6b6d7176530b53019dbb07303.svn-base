package com.bcwms.bean.crew.translate;

import com.keepjoy.core.util.DateUtil;

import java.util.Date;

public class CrewTranslateBean {



    public String getBeginDateTimeYYYYMMDD(Date beginDateTime){
        return DateUtil.dateToStringByDateFormat(beginDateTime,DateUtil.DATEFORMAT_YYYY_MM_DD_HH_MM_SS);
    }

    public String getEndDateTimeYYYYMMDDHHMMSS(Date endDateTime){
        return DateUtil.dateToStringByDateFormat(endDateTime,DateUtil.DATEFORMAT_YYYY_MM_DD_HH_MM_SS);
    }

}
