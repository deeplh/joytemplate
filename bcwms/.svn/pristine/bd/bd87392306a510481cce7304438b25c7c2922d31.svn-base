package com.keepjoy.security.bean.user;

import java.util.Map;

public class MenuBean implements java.io.Serializable{

	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	private Integer id;
	private String menuName;
	private String menuUrl;
	private String parentId;
	
	private Map<Integer,ResourceBean> resources;

	public Integer getId() {
		return id;
	}

	public void setId(Integer id) {
		this.id = id;
	}

	public String getMenuName() {
		return menuName;
	}

	public void setMenuName(String menuName) {
		this.menuName = menuName;
	}

	public String getMenuUrl() {
		return menuUrl;
	}

	public void setMenuUrl(String menuUrl) {
		this.menuUrl = menuUrl;
	}
	
	public String getParentId() {
		return parentId;
	}

	public void setParentId(String parentId) {
		this.parentId = parentId;
	}

	public Map<Integer, ResourceBean> getResources() {
		return resources;
	}

	public void setResources(Map<Integer, ResourceBean> resources) {
		this.resources = resources;
	}

	
	
}
